{
  "name": "@types/simple-git",
  "main": "./src/index.d.ts",
  "version": "1.54.0",
  "private": true,
  "scripts": {
    "lint": "echo linting... && tslint \"**/*.ts\" -e \"source/**\" -e \"out/**\" -e \"node_modules/**\" -e \"typings/**\"",
    "test": "echo running server tests... && cd test && ts-node ../node_modules/blue-tape/bin/blue-tape \"**/*.ts\" | tap-spec",
    "watch": "onchange -w \"**/*.ts\" -i -e \"out/**\" -- npm run test+lint",
    "publish": "npm -s run test+lint",
    "all-tests": "npm test",
    "test+lint": "npm run all-tests && npm run lint"
  },
  "devDependencies": {
    "@types/blue-tape": "^0.1.31",
    "@types/node": "^8.0.25",
    "blue-tape": "^1.0.0",
    "onchange": "^3.2.1",
    "simple-git": "^1.75.0",
    "tap-spec": "^4.1.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "tslint-config-typings": "^0.3.1"
  }
}
